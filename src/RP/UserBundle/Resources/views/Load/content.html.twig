    <div class="tab-pane fade in active page-content" id="tab1">
      <div class="container"> 
        <div class="row">
   
          <div class="col-md-9 posts">  
          {% if listActualite|length > 0  %}               
            {% for actualite in listActualite %} 
              {% if actualite.target != user and actualite.type == "Statut" %}
              {% else %}
                {% if actualite.user == user  or actualite.target == user %}
                  {% if actualite.type == "message" %}   
                    <div class="[ col-xs-12 col-sm-offset-1 col-md-9 ]">
                      <div class="[ panel panel-default ] panel-google-plus">
                        <div class="dropdown">
                          <span class="dropdown-toggle" type="button" data-toggle="dropdown">
                            <span class="[ glyphicon glyphicon-chevron-down ]"></span>
                          </span>
                          <ul class="dropdown-menu" role="menu">
                            <li role="presentation"><a role="menuitem" onclick="deleteActu({{actualite.id}})" tabindex="-1" href="#">Supprimer</a></li>
                          </ul>
                        </div>
                        <div class="panel-google-plus-tags">
                          <ul>
                            <li>+ {{actualite.type}}</li>
                          </ul>
                        </div>
                        <div class="panel-heading">
                          <img class="[ img-circle pull-left ]" src="{{ asset('images/') }}{{actualite.user.imagename}}" width="70px" height="70px" alt="profile" />
                          <h3><a href='../user/{{actualite.user.id}}'>{{actualite.user.username}}</a></h3>
                          <h5><span>Actualite</span> - <span>{{actualite.date|date("h:m")}}</span> - <span>{{actualite.date|date("d/m/Y")}}</span> </h5>
                        </div>
                        <div class="panel-body">
                          <p>[{{actualite.target.username}}] {{actualite.contenu | raw}}</p> 
                          <p><a class="tool" onclick="comments({{actualite.id}});">Commentaires</a></p>
                          <div class="box_{{actualite.id}}">
                            <div class="commentbox"> 
                            </div>
                          </div>
                        </div>
                        <div class="panel-footer">
                          <!--<button onclick="like({{actualite.id}})" type="button" class="[ btn ] act">
                              <span class="[ glyphicon glyphicon-heart ]"></span>
                          </button>-->  
     
                         <input class="input-comment col-lg-9  text_{{actualite.id}}" type="text"  placeholder ="Add a comment..." > 

                          <button  onclick="comment({{actualite.id}})" type="button" class="[ btn ] act ">
                            <span class="[ glyphicon glyphicon-share-alt ]"></span>
                          </button>
                         
                        </div>
                        <div class="panel-google-plus-comment">
                          <img class="img-circle" src="https://lh3.googleusercontent.com/uFp_tsTJboUY7kue5XAsGA=s46" alt="User Image" />
                          <div class="panel-google-plus-textarea"> 
                            <textarea rows="4"></textarea>
                            <button onclick="comment({{actualite.id}})" type="submit" class="[ btn btn-success disabled ]">Commenter</button>
                            <button type="reset" class="[ btn btn-default ]">Cancel</button>
                          </div>
                          <div class="clearfix"></div>
                        </div>
                      </div>
                    </div>
                  {% else %}
                    <div class="[ col-xs-12 col-sm-offset-1 col-md-9 ]">
                      <div class="[ panel panel-default ] panel-google-plus">
                        <div class="dropdown">
                          <span class="dropdown-toggle" type="button" data-toggle="dropdown">
                            <span class="[ glyphicon glyphicon-chevron-down ]"></span>
                          </span>
                          <ul class="dropdown-menu" role="menu">
                            <li role="presentation"><a role="menuitem" onclick="deleteActu({{actualite.id}})" tabindex="-1" href="#">Supprimer</a></li>
                          </ul>
                        </div>
                        <div class="panel-google-plus-tags">
                          <ul>
                            <li>+ {{actualite.type}}</li>
                          </ul>
                        </div>
                        <div class="panel-heading">
                          <img class="[ img-circle pull-left ]" src="{{ asset('images/') }}{{actualite.user.imagename}}" width="70px" height="70px" alt="profile" />
                          <h3><a href='../user/{{actualite.user.id}}'>{{actualite.user.username}}</a></h3>
                          <h5><span>Actualite</span> - <span>{{actualite.date|date("h:m")}}</span> - <span>{{actualite.date|date("d/m/Y")}}</span> </h5>
                        </div>
                        <div class="panel-body">
                          <p>{{actualite.contenu | raw}}</p> 
                          <p><a class="tool" onclick="comments({{actualite.id}});">Commentaires</a></p>
                          <div class="box_{{actualite.id}}">
                            <div class="commentbox"> 
                            </div>
                          </div>
                        </div>
                        <div class="panel-footer">
                          <!--<button onclick="like({{actualite.id}})" type="button" class="[ btn ] act">
                              <span class="[ glyphicon glyphicon-heart ]"></span>
                          </button>-->  
     
                         <input class="input-comment col-lg-9  text_{{actualite.id}}" type="text"  placeholder ="Add a comment..." > 

                          <button  onclick="comment({{actualite.id}})" type="button" class="[ btn ] act ">
                            <span class="[ glyphicon glyphicon-share-alt ]"></span>
                          </button>
                         
                        </div>
                        <div class="panel-google-plus-comment">
                          <img class="img-circle" src="https://lh3.googleusercontent.com/uFp_tsTJboUY7kue5XAsGA=s46" alt="User Image" />
                          <div class="panel-google-plus-textarea"> 
                            <textarea rows="4"></textarea>
                            <button onclick="comment({{actualite.id}})" type="submit" class="[ btn btn-success disabled ]">Commenter</button>
                            <button type="reset" class="[ btn btn-default ]">Cancel</button>
                          </div>
                          <div class="clearfix"></div>
                        </div>
                      </div>
                    </div>
                  {% endif %} 
                {% endif %}
              {% endif %}
            {% endfor %} 
          {% else %}
                <div class="[ col-xs-12 col-sm-offset-1 col-md-9 ]">
                  <div class="[ panel panel-default ] panel-google-plus">
                    <div class="dropdown">
                      <span class="dropdown-toggle" type="button" data-toggle="dropdown">
                        <span class="[ glyphicon glyphicon-chevron-down ]"></span>
                      </span>
                    </div>
                    <div class="panel-google-plus-tags">
                      <ul>
                        <li>+ Actualit√©</li>
                      </ul>
                    </div>
                    <div class="panel-heading">
                    <img class="[ img-circle pull-left ]" src="{{ asset('bundles/rpcore/img/anime.jpg') }}" width="70px" height="70px" alt="profile" />
                      <h3>Equipe Ocameo</h3>
                    </div>
                    <div class="panel-body">
                      <p>Bienvenue sur Ocameo ! </p> 
                    </div>
                    <div class="panel-footer">
                    </div>
                  </div>
                </div>
          {% endif %}  
             <div class="[ col-xs-12 col-sm-offset-5 col-md-9 ]">     
              <a class="btn btn-primary" onclick="more()" >Voir plus </a>
             </div>
          </div>
          <div class="col-md-3 col-side hidden-sm">
            <aside class="blog-widget-area widget-area">    
              <div id="search-2" class="widget widget_search">
              </div>      
              <div id="recent-posts-2" class="widget widget_recent_entries"><h3 class="widget-title">Dernieres series </h3>      
                <ul>
                  {% for serie in listSeries %}
                    <li>
                      <a href="http://demo.themesholic.com/flex/standard-post-2/">{{serie.name}}</a>
                      <span class="post-date">{{serie.type}}</span>
                    </li>
                  {% endfor %}
                </ul>
              </div>
              <div id="categories-2" class="widget widget_categories"><h3 class="widget-title">Categories</h3>      
                <ul>
                  {% for type in types %}
                    <li class="cat-item cat-item-17"><a href="http://demo.themesholic.com/flex/category/media/" >{{type.type}}</a></li> 
                  {% endfor %}
                </ul>
              </div>
              <div id="md_widget_pinterest-2" class="widget widget_md_widget_pinterest">
                <h3 class="widget-title">Plus de series</h3>
                <ul>
                  {% for serie in listSeries %}
                    <li style="background-image:url({{serie.image}}"><a href="https://www.pinterest.com/pin/212091463678746524/" target="_blank"></a></li>
                  {% endfor %}
                </ul>
              </div>    
            </aside>
          </div>         
        </div>      
      </div>
    </div>
    <div class="tab-pane fade in" id="tab2">
      <div class="container"> 
        <div class="row">
          <div class="col-md-9 posts">
            {% for demande in listDemandes %}
              {% if demande.statut == "waiting" %}
                <div class="[ col-xs-12 col-sm-offset-1 col-md-9 ] ask_{{demande.id}}">
                  <div class="[ panel panel-default ] panel-google-plus">
                    <div class="panel-heading">
                      <img class="[ img-circle pull-left ]" src="https://lh3.googleusercontent.com/-CxXg7_7ylq4/AAAAAAAAAAI/AAAAAAAAAQ8/LhCIKQC5Aq4/s46-c-k-no/photo.jpg" alt="Mouse0270" />
                      <h3><a href='../user/{{demande.holder.id}}'>{{demande.holder.username}}</a> t'as ajout√© en ami</h3>
                      <h5><span>Demande d'amis</span> - <span>{{demande.date|date("h:m")}}</span> - <span>{{demande.date|date("d/m/Y")}}</span> </h5>
                    </div>
                    <div class="panel-footer">
                      <button type="button" class="[ btn ] btn-primary" onclick="accept({{demande.id}});">Accepter</button>
                      <button type="button" class="[ btn ] btn-danger" onclick="refuse({{demande.id}});">Refuser</button>
                    </div>
                  </div>
                </div>
              {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>
    </div>